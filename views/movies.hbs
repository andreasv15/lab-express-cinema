<h1> Ironhack Cinema</h1>

<h3> Click on the movie to check the showtimes!</h3>


<section id="listMovies">
    {{#each listMovies}}
        <article class="eachMovie">
                <img src="{{this.image}}" alt="pelicula" />
                <p class="titleMovie">{{this.title}} </p>
                <p> <a href="/movie/{{this.id}}">See more</a> </p>
        </article>
    {{/each}}
</div>
